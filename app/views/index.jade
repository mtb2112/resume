doctype html
html
    head
        title
        - var cssRoot = 'css';
        - var jsRoot = 'js';
        link(rel='stylesheet', href='#{cssRoot}/skeleton.css')
        link(rel='stylesheet', href='#{cssRoot}/global.css')
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css')
    body(data-spy="scroll", data-target=".navbar")
        include ../../images/svg-defs.svg
        div#top.profile-banner(data-position='center right')
            div.container
                h1.author #{resume.user.name}
                p.sub #{resume.user.resumeTitle}
            div.photo-overlay
            a.scroll-down.fa(href='#profile')
                i.fa.fa-level-down
        nav.navbar.navbar-default#navbar-example(role='navigation')
            div.main-nav
                ul.nav.navbar-nav
                    li.menu-item.active
                        a.menu-link(href='#profile') Profile
                    li.menu-item
                        a.menu-link(href='#experience') Experience
                    li.menu-item
                        a.menu-link(href='#abilities') Abilities
                    li.menu-item
                        a.menu-link(href='#contact') Contact
        div#profile.container
            h2.section-heading Profile
            p.sub
            hr
            div.row
                div.one-third.column
                    h3.sub-heading About Me
                    p #{resume.user.description}
                div.one-third.column.text-center
                    div.profile-photo-wrap
                        img.profile-photo(src='images/guardinopaul.jpg', alt='Paul Guardino')
                div.one-third.column
                    h3.sub-heading Details
                    p
                        strong Name:
                        br
                        | #{resume.user.name}
                        br
                        strong Age:
                        br
                        |#{resume.user.age}
                        br
                        strong Location:
                        br
                        |#{resume.user.location}
        div.experience-full
            div#experience.container
                h2.section-heading Experiences
                hr
                h3.sub-heading Education
                div.experiences
                    each role, i in resume.educationTimeline
                        div.experience.row
                            div.four.columns
                                h4 #{role.company}
                                p.experience-duration #{role.start} - #{role.end}
                            div.eight.columns
                                p
                                    strong #{role.position}
                                    span.experience-description #{role.description}
                                    span.experience-information
                                        i.fa.fa-map-marker
                                            span.experience-location #{role.location}
                                        i.fa.fa-external-link
                                            span.experience-link
                                                a(href='#{role.url}', target='_blank') #{role.url}
                hr
                h3.sub-heading Careers
                div.experiences
                    each role, i in resume.workTimeline
                        div.experience.row
                            div.four.columns
                                h4 #{role.company}
                                p.experience-duration #{role.start} - #{role.end}
                            div.eight.columns
                                p
                                    strong #{role.position}
                                    span.experience-description #{role.description}
                                    span.experience-information
                                        i.fa.fa-map-marker
                                            span.experience-location #{role.location}
                                        i.fa.fa-external-link
                                            span.experience-link
                                                a(href='#{role.url}', target='_blank') #{role.url}
